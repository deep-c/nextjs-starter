{
  "name": "spa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run build",
    "dev": "dotenv -c -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:prettier": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "test": "jest",
    "db:generate": "dotenv -c -- prisma generate",
    "db:makemigration": "dotenv -c -- prisma migrate dev"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  },
  "dependencies": {
    "@apollo/client": "3.3.20",
    "@headlessui/react": "1.3.0",
    "@heroicons/react": "1.0.1",
    "@next-auth/prisma-adapter": "0.4.4-canary.64",
    "@prisma/client": "2.25.0",
    "@tailwindcss/forms": "0.3.3",
    "apollo-server-micro": "2.25.2",
    "graphql": "15.5.1",
    "next": "11.0.1",
    "next-auth": "3.27.0",
    "nexus": "1.1.0",
    "nexus-prisma": "0.29.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.10.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "11.0.1",
    "@types/react": "17.0.11",
    "autoprefixer": "10.2.6",
    "cross-env": "7.0.3",
    "dotenv-cli": "4.0.0",
    "eslint": "7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.0.5",
    "postcss": "8.3.5",
    "prettier": "2.3.1",
    "prisma": "2.25.0",
    "tailwindcss": "2.2.4",
    "ts-node": "10.0.0",
    "typescript": "4.3.4"
  }
}
